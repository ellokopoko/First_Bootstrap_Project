.content.row
  .col-lg-12
    %header.clearfix
      %section#branding
        %a{href: "index.php"}
          %img{src: "images/misc/ralogo_monogram.png", alt: "Logo for Roux Conference"}
      .navbar.navbar-default
        .container-fluid
          %ul.nav.navbar-nav.navbar-right
            - pages = [{name: "Home", url:"index.php"}, {name: "Venue/Travel", url:"venuetravel.php"}, {name: "Schedule", url:"schedule.php"}, {name: "Artists", url:"artists.php"}, {name: "Register", url:"register.php"}]
            - artists = ["Barot Bellingham", "Gerard Donahue", "Jonathan Ferrar", "Lorenzo Garcia", "Hillary Goldwynn", "Hassum Harrod", "Jennifer Jerome", "LaVonne LaRue", "Riley Rewington", "Constance Smith", "Xhou Ta", "Richard Tweed"]
            - for page in pages
              - if page.name == "Artists"

                %li{class: "dropdown"} 
                  %a{href: "#{page.url}", class: "dropdown-toggle", "data-toggle": "dropdown"} = page.name
                    %b{class: "caret"}
                  %ul{class: "dropdown-menu", rol: "menu", "aria-labelledby": "dropdownMenu"}
                    %li
                      %a{href: "artists.php", tabindex: "-1"} All artists
                    %li.divider
                    - for artist in artists
                      %li
                        %a{href: "artists.php##{artist.slice(0, artist.indexOf(" ")) + "_" + artist.slice(artist.indexOf(" ") + 1)}", tabindex: "-1"} =artist

              - else
                %li
                  %a{href: "#{page.url}"} = page.name   
                  